
@*Wave Planning*@



<div id="@("VAWMS_MainDiv_" + ViewBag.WindowNumber)" class="waveMain-div">
    @*1st row*@
    <div class="param-div">
        <label  id="@("VAMFG_WHLabel_" + ViewBag.WindowNumber)" >Warehouse</label>
        <select id="@("VAMFG_WHCombo_" + ViewBag.WindowNumber)" ></select>
    </div>
    <div class="param-div">
        <label  id="@("VAMFG_WOTypeLbl_" + ViewBag.WindowNumber)" >Work Order Type Group</label>
        <select id="@("VAMFG_WOTypeCombo_" + ViewBag.WindowNumber)"></select>
    </div>
    <div class="param-div">
        <label  id="@("VAMFG_WOLabel_" + ViewBag.WindowNumber)">Work Order</label>
        <select id="@("VAMFG_WOCombo_" + ViewBag.WindowNumber)"></select>
