    </div>
    <div class="param-div">
        <label  id="@("VAMFG_OpertnlSheduledDateToLbl_" + ViewBag.WindowNumber)">Opr. Scheduled Date To</label>
        <input  id="@("VAMFG_OpertnlSheduledDateToCtrl_" + ViewBag.WindowNumber)"  type="date"  class="date-ctrl"/>
